{
  "name": "ethers-react-demo",
  "author": "Luis Osta",
  "homepage": "https://github.com/rapid-eth/rapid-playground/tree/master",
  "description": "Demo application of the Ethers Provider component library",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "eslint ./src/**/*.js --max-warnings=0 --format=codeframe",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build",
    "preproduction": "yarn run build",
    "production": "gh-pages -o production -d build",
    "ganache": "ganache-cli -p 8545 -q"
  },
